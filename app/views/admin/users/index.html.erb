<div class="row">
  <div class="col-md-10">
    <h2>Users List</h2> 
  </div>
</div>      
<table class="table table-striped">
<thead>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Email</th>
    <th>Action</th>
  </tr>
</thead>
<tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.full_name %></td>
      <td><%= user.email %></td>
      <td><%= link_to "Edit", edit_admin_user_path(user),  data: { turbolinks: false }, class: "btn btn-primary" %></td>
    </tr>
  <% end %>
</tbody>
</table>