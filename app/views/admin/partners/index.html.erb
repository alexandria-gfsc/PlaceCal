<div class="row">
  <div class="col-md-10">
    <h2>Partners List</h2> 
  </div>
  <div class="col-md-2">
    <%= link_to "Add New Partner", new_admin_partner_path, class: "btn btn-primary " %>   
  </div>
</div>      
<table class="table table-striped">
<thead>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Email</th>
    <th>Action</th>
  </tr>
</thead>
<tbody>
  <% @partners.each do |partner| %>
    <tr>
      <td><%= partner.id %></td>
      <td><%= partner.name %></td>
      <td><%= partner.public_email %></td>
      <td><%= link_to "Edit", edit_admin_partner_path(partner.id), class: "btn btn-primary " %></td>
    </tr>
  <% end %>
</tbody>
</table>